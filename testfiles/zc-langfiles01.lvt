\usepackage{zref-clever}

% This file is intended to test the build-in language files.

\START

\ExplSyntaxOn

\TEST { Built-in~language~files }
  {
    \seq_gclear:N \g__zrefclever_loaded_langfiles_seq
    \bool_set_false:N \l_tmpa_bool
    \prop_map_inline:Nn \g__zrefclever_languages_prop
      {
        \str_if_eq:nnT {#1} {#2}
          {
            \bool_if:nTF \l_tmpa_bool
              { \SEPARATOR }
              { \bool_set_true:N \l_tmpa_bool }
            \__zrefclever_provide_langfile_verbose:n {#2}
            \prop_show:c { g__zrefclever_lang_ #2 _prop}
          }
      }
  }

\OMIT
\begin{document}
\TIMO

\TEST { Fallback }
  {
    \prop_show:N \g__zrefclever_fallback_unknown_lang_prop
  }

\OMIT

\end{document}
